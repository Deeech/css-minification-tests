{
  "name": "css-minificator-tests",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run clean && node ./bin/run",
    "clean": "rm -rf site/ gemini/screens gemini-report dist/*",
    "build": "cp -r site dist/site && cp -r gemini dist/gemini && cp -r gemini-report dist/gemini-report",
    "deploy": "gh-pages -d dist"
  },
  "main": "lib/run",
  "repository": {
    "url": "git@github.com:Deeech/css-minificator-tests.git",
    "type": "git"
  },
  "keywords": [
    "css",
    "minification",
    "test"
  ],
  "author": "Eugene Zharov <deeech88@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "clean-css": "^3.4.20",
    "crass": "^0.7.12",
    "css-condense": "^0.1.1",
    "cssnano": "^3.8.0",
    "csso": "^2.2.1",
    "cssshrink": "^0.0.5",
    "csswring": "^5.1.0",
    "es6-promise": "^4.0.5",
    "gemini": "^4.13.0",
    "gzip-size": "^3.0.0",
    "handlebars": "^4.0.6",
    "more-css": "^0.12.0",
    "ncss": "^1.1.1",
    "open": "0.0.5",
    "q": "^1.4.1",
    "sqwish": "^0.2.2",
    "website-scraper": "^1.2.0",
    "ycssmin": "^1.0.1"
  }
}
